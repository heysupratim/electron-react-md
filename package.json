{
  "name": "react-ts-electron-vite",
  "private": true,
  "version": "0.0.0",
  "main": "app/index.js",
  "description": "Boilerplate Electron Forge + Vite + React + Typescript",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k 'vite' 'yarn electron-forge:dev'",
    "build": "tsc && vite build",
    "electron-forge:dev": "cross-env IS_DEV=true electron-forge start",
    "start": "yarn build && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "22.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
