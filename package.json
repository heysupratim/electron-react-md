{
  "name": "electronreactmd",
  "private": true,
  "version": "0.0.0",
  "main": "app/index.js",
  "description": "Boilerplate Electron Markdown Edit and Preview with Forge + Vite + React + Typescript + Remark + Rehype",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k 'vite' 'yarn electron-forge:dev'",
    "build": "tsc && vite build",
    "electron-forge:dev": "cross-env IS_DEV=true electron-forge start",
    "start": "yarn build && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@codemirror/commands": "^6.1.2",
    "@codemirror/lang-javascript": "^6.1.2",
    "@codemirror/lang-markdown": "^6.0.5",
    "@codemirror/language": "^6.3.2",
    "@codemirror/language-data": "^6.1.0",
    "@codemirror/state": "^6.1.4",
    "@codemirror/theme-one-dark": "^6.1.0",
    "@codemirror/view": "^6.7.1",
    "@lezer/highlight": "^1.1.3",
    "electron-squirrel-startup": "^1.0.0",
    "github-markdown-css": "^5.1.0",
    "highlight.js": "^11.7.0",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-highlight": "^6.0.0",
    "rehype-react": "^7.1.1",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "electron": "22.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
